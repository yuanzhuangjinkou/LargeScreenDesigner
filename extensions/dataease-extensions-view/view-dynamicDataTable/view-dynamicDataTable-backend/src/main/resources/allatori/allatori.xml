<config>
    <input>
        <jar in="../../*.jar" out="../../*.jar"/>
    </input>

    <ignore-classes>
        <class template="class org.*"/>
    </ignore-classes>

    <keep-names>
        <class template="class io.dataease.plugins.view.official.*" access="private+">
            <field access="protected+"/>
            <method access="protected+"/>
        </class>
    </keep-names>

    <property name="log-file" value="log.xml"/>
</config>